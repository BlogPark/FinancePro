                        <td height="22">@item.LogRemark</td>
                        <td height="22">@item.MemberName</td>
                        <td height="22">@item.NMemberPoints</td>
                        <td height="22">@item.NCompoundCurrency</td>
                        <td height="22">@item.NGameCurrency</td>
                        <td height="22">@item.NShoppingCurrency</td>
                        <td>@item.NSharesCurrency</td>
                    </tr>
                }

                
                <tr>
                    <td height="28" colspan="10" align="right">
                        <label></label>
                        @Html.Pager(Model.loglist, new PagerOptions { PageIndexParameterName = "page", ShowPageIndexBox = false, PageIndexBoxType = PageIndexBoxType.DropDownList, ShowGoButton = false })
