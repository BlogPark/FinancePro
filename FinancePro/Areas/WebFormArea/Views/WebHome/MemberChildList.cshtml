@model FinancePro.Areas.WebFormArea.Models.MemberChildListViewModel
@{
    ViewBag.Title = "会员子账户管理";
    Layout = "~/Views/_WebFormLayout.cshtml";
}

<table width="871" border="0" cellspacing="0" cellpadding="0">
    <tr>
        <td width="11" align="left"><img src="/webasset/images/index-2-l.jpg" width="11" height="36" /></td>
        <td width="204" align="left" background="/webasset/images/index-2-bj.jpg"><strong><font style="font-size:14px" color="#000000">会员管理</font></strong></td>
        <td width="17" align="left"><img src="/webasset/images/index-2-2.jpg" width="17" height="36" /></td>
        <td align="left" background="/webasset/images/index-2-bj.jpg"><strong><font style="font-size:14px" color="#000000">待激活会员列表</font></strong></td>
        <td width="10" align="right"><img src="/webasset/images/index-2-3.jpg" width="10" height="36" /></td>
    </tr>
</table>
<table width="871" height="303" border="0" cellpadding="0" cellspacing="0" background="/webasset/images/bj2.jpg">
    <tr>
        <td width="11">&nbsp;</td>
        <td width="204" align="left" valign="top">
            @Html.Action("LeftWebMenu", "IndexPub", new { area = "", type = 2 })
        </td>
        <td width="13" align="left" valign="top">&nbsp;</td>
        <td width="643" align="center" valign="top">
            <table width="97%" border="0" cellspacing="0" cellpadding="0">
                <tr>
                    <td>&nbsp;</td>
                </tr>
            </table>
            <table width="96%" border="1" cellpadding="5" cellspacing="0" bordercolor="#666666" style="border-collapse:collapse;">
                <tr>
                    <td width="13%" align="center"><strong>会员编号</strong></td>
                    <td width="12%" align="center"><strong>真实姓名</strong></td>
                    <td width="12%" align="center"><strong>手机号码</strong></td>
                    <td width="12%" align="center"><strong>身份证号</strong></td>
                    <td width="12%" align="center"><strong>状态</strong></td>
                    <td width="12%" align="center"><strong>注册时间</strong></td>
                    <td width="15%" align="center"><strong>操作</strong></td>
                </tr>
                @foreach (var item in Model.memberlist)
                {
                    <tr>
                        <td height="22">@item.MemberCode</td>
                        <td height="22">@item.MemberName</td>
                        <td height="22">@item.MemberPhone</td>
                        <td height="22">@item.MemberIDNumber</td>
                        <td height="22">@item.MemberStatusName</td>
                        <td height="22">@item.AddTime.ToString("yyyy-MM-dd HH:mm")</td>
                        <td height="22">
                            @if (item.MemberStatus == 1)
                            {
                                <input type="button" name="Submit2" onclick="activemember(@item.ID)" value="激活" />
                            }
                        </td>
                    </tr>
                }
            </table>
            <table width="97%" border="0" cellspacing="0" cellpadding="0">
                <tr>
                    <td height="49" align="right">&nbsp;</td>
                </tr>
            </table>
        </td>
    </tr>
</table>
