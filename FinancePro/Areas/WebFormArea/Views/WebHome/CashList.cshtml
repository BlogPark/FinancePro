@using Webdiyer.WebControls.Mvc
@model FinancePro.Areas.WebFormArea.Models.CashListViewModel
@{
    ViewBag.Title = "提现列表";
    Layout = "~/Views/_WebFormLayout.cshtml";
}

<table width="871" border="0" cellspacing="0" cellpadding="0">
    <tr>
        <td width="11" align="left"><img src="/webasset/images/index-2-l.jpg" width="11" height="36" /></td>
        <td width="204" align="left" background="/webasset/images/index-2-bj.jpg"><strong><font style="font-size:14px" color="#000000">财务管理</font></strong></td>
        <td width="17" align="left"><img src="/webasset/images/index-2-2.jpg" width="17" height="36" /></td>
        <td align="left" background="/webasset/images/index-2-bj.jpg"><strong><font style="font-size:14px" color="#000000">提现列表</font></strong></td>
        <td width="10" align="right"><img src="/webasset/images/index-2-3.jpg" width="10" height="36" /></td>
    </tr>
</table>
<table width="871" height="303" border="0" cellpadding="0" cellspacing="0" background="/webasset/images/bj2.jpg">
    <tr>
        <td width="11">&nbsp;</td>
        <td width="204" align="left" valign="top">
            @Html.Action("LeftWebMenu", "IndexPub", new { area = "", type = 1 })
        </td>
        <td width="13" align="left" valign="top">&nbsp;</td>
        <td width="643" align="center" valign="top">
            <table width="97%" border="0" cellspacing="0" cellpadding="0">
                <tr>
                    <td>&nbsp;</td>
                </tr>
            </table>
            <table width="96%" border="1" cellpadding="5" cellspacing="0" bordercolor="#666666" style="border-collapse:collapse;">
                <tr>
                    <td width="4%" height="28" align="center">&nbsp;</td>
                    <td width="8%" align="center"><strong>提现类型</strong></td>
                    <td width="8%" align="center"><strong>收款银行</strong></td>
                    <td width="9%" align="center"><strong>银行卡号</strong></td>
                    <td width="21%" align="center"><strong>开户行</strong></td>
                    <td width="7%" align="center"><strong>提现金额</strong></td>
                    <td width="7%" align="center"><strong>手续费</strong></td>
                    <td width="7%" align="center"><strong>实付金额</strong></td>
                    <td width="8%" align="center"><strong>备注</strong></td>
                    <td width="8%" align="center"><strong>状态</strong></td>
                    <td width="13%" align="center"><strong>申请时间</strong></td>
                </tr>
                @foreach (var item in Model.cashlist)
                {
                    <tr>
                        <td height="22" align="center">@item.ID</td>
                        <td height="22">@item.CashOrderCode</td>
                        <td height="22">@item.CashBankName</td>
                        <td height="22">@item.CashBankUserName</td>
                        <td height="22">@item.CashBankCode</td>
                        <td height="22">@item.CashNum</td>
                        <td height="22">@item.CommissionNum</td>
                        <td height="22">@item.FinishCashNum</td>
                        <td height="22">@item.DiffDay</td>
                        <td>@item.CStatusName</td>
                        <td>@item.AddTime.ToString("yyyy-MM-dd HH:mm")</td>
                    </tr>
                }

                
                <tr>
                    <td height="28" colspan="11" align="right">
                        <label></label>
                        @Html.Pager(Model.cashlist, new PagerOptions { PageIndexParameterName = "page", ShowPageIndexBox = false, PageIndexBoxType = PageIndexBoxType.DropDownList, ShowGoButton = false })
                    </td>
                </tr>
            </table>
            <table width="97%" border="0" cellspacing="0" cellpadding="0">
                <tr>
                    <td height="49" align="right">&nbsp;</td>
                </tr>
            </table>
        </td>
    </tr>
</table>

